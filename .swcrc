{
 "$schema": "https://json.schemastore.org/swcrc",
 "jsc": {
  "parser": {
    "syntax": "typescript",
    "tsx": true,
    "dynamicImport": true
  },
  "transform": {
    "react": {
      "runtime": "automatic"
    }
  },
  "minify": {
    "compress": true,
    "mangle": true
  },
  "target": "esnext"
 },
 "minify": true,
 "module": {
  "type": "es6"
 }
}

