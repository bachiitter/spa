  "author": "Bachitter",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "check": "pnpm lint:check && pnpm format:check && pnpm typecheck",
    "dev": "webpack serve",
    "fix": "pnpm lint:fix && pnpm format:fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,cjs,mjs,json,md,mdx}\" --cache --ignore-path .gitignore",
    "format:fix": "prettier --write \"**/*.{ts,tsx,cjs,mjs,json,md,mdx,html}\" --cache --ignore-path .gitignore",
    "lint:check": "eslint . --cache --cache-strategy content --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "start": "serve dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/router": "0.0.1-beta.86",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@fontsource/inter": "^4.5.15",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@swc/core": "^1.3.58",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "serve": "^14.2.0",
    "swc-loader": "^0.2.3",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "^3.3.2",
    "terser-webpack-plugin": "^5.3.8",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
